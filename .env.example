# Example .env file for RIFT'26 DevOps Project
# Copy this to .env and fill in your values

# Git Configuration
GIT_USER_NAME=DevOps Lead
GIT_USER_EMAIL=devops@rift26.com

# Repository Settings
DEFAULT_BRANCH=main
REMOTE_NAME=origin
REPO_PATH=.

# CI/CD Configuration
CI_ENVIRONMENT=local
PIPELINE_ITERATIONS=3
PIPELINE_DELAY=5

# Docker Configuration
DOCKER_IMAGE_NAME=rift26-ci
DOCKER_CONTAINER_NAME=rift26-pipeline

# Deployment Configuration - Railway
RAILWAY_PROJECT_ID=your-project-id
RAILWAY_ENVIRONMENT=production

# Deployment Configuration - AWS
AWS_REGION=us-east-1
AWS_SERVICE_TYPE=lambda
AWS_FUNCTION_NAME=rift26-backend
AWS_ACCOUNT_ID=your-account-id

# Logging
LOG_LEVEL=INFO
LOG_DIR=logs

# Data Directory
DATA_DIR=data

# Branch Configuration
BRANCH_PREFIX=TEAM_LEADER_AI_Fix
